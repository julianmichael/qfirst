{
  "dataset_reader": {
      "type": "qfirst_qasrl",
      "instance_type": "question",
      "token_indexers": {
          "tokens": {
            "type": "single_id",
            "lowercase_tokens": true
          }
      }
 },
  // "train_data_path": "dev-mini.jsonl",
  // "validation_data_path": "dev-mini.jsonl",
  // "test_data_path": "dev-mini.jsonl",
  "train_data_path": "http://qasrl.org/data/qasrl-v2/orig/train.jsonl.gz",
  "validation_data_path": "http://qasrl.org/data/qasrl-v2/orig/dev.jsonl.gz",
  "test_data_path": "http://qasrl.org/data/qasrl-v2/orig/test.jsonl.gz",
    "model": {
        "type": "noctx_conditional_lm",
        "pred_hidden_dim": 100,
        "text_field_embedder": {
            "tokens": {
                "type": "embedding",
                "embedding_dim": 100,
                "pretrained_file": "https://s3-us-west-2.amazonaws.com/allennlp/datasets/glove/glove.6B.100d.txt.gz",
                "trainable": false
            }
        },
        "question_model": {
            "type": "sequential",
            "slot_names": ["abst-wh", "abst-subj", "abst-verb", "abst-obj", "prep", "abst-obj2"],
            "dim_slot_hidden":100,
            "dim_rnn_hidden": 200,
            "input_dim": 100,
            "rnn_layers": 4,
            "share_rnn_cell": false
        },
    },
  "iterator": {
    "type": "bucket",
    "sorting_keys": [["text", "num_tokens"]],
    "batch_size" : 80
  },
  "trainer": {
    "num_epochs": 200,
    "grad_norm": 1.0,
    "patience": 5,
    "validation_metric": "-perplexity-per-question",
    "cuda_device": -1,
    "optimizer": {
      "type": "adadelta",
      "rho": 0.95
    }
  }
}
