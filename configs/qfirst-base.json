{
  "dataset_reader": {
      "type": "qasrl",
      "token_indexers": {
          "tokens": {
            "type": "single_id",
            "lowercase_tokens": true
          },
          "elmo": {
            "type": "elmo_characters"
          }
      }
 },
  "train_data_path": "http://qasrl.org/data/qasrl-v2/orig/train.jsonl.gz",
  "validation_data_path": "http://qasrl.org/data/qasrl-v2/orig/dev.jsonl.gz",
  "test_data_path": "http://qasrl.org/data/qasrl-v2/orig/test.jsonl.gz",
    "model": {
        "type": "qfirst_parser",
        "qfirst_generator": {
            "type": "qfirst_conditional_lm",
            "text_field_embedder": {
                "elmo":{
                    "type": "elmo_token_embedder",
                    "options_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_options.json",
                    "weight_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_weights.hdf5",
                    "do_layer_norm": false,
                    "dropout": 0.5
                }
            },
            "stacked_encoder": {
                "type": "alternating_lstm",
                "use_highway": true,
                "input_size": 1124,
                "hidden_size": 300,
                "num_layers": 4,
                "recurrent_dropout_probability": 0.1
            },
            "predicate_feature_dim":100,
            "hidden_dim":100,
            "question_generator": {
                "type": "sequence",
                "slot_labels": ["WH", "AUX", "SBJ", "TRG", "OBJ1", "PP", "OBJ2"],
                "dim_slot_hidden":100,
                "dim_rnn_hidden": 200,
                "input_dim": 300,
                "rnn_layers": 4,
                "share_rnn_cell": false
            },
        },
        "question_answerer": {
            "type": "qfirst_question_encoding_answerer",
            "text_field_embedder": {
                "elmo":{
                    "type": "elmo_token_embedder",
                    "options_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_options.json",
                    "weight_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_weights.hdf5",
                    "do_layer_norm": false,
                    "dropout": 0.5
                }
            },
            "stacked_encoder": {
                "type": "alternating_lstm",
                "use_highway": true,
                "input_size": 1124,
                "hidden_size": 300,
                "num_layers": 4,
                "recurrent_dropout_probability": 0.1
            },
            "predicate_feature_dim":100,
            "hidden_dim":100,
            "question_encoder": {
                "type": "sequence",
                "slot_labels": ["WH", "AUX", "SBJ", "TRG", "OBJ1", "PP", "OBJ2"],
                "dim_slot_hidden":100,
                "dim_rnn_hidden": 200,
                "input_dim": 300,
                "rnn_layers": 4,
                "share_rnn_cell": false
            },
        },
    },
  "iterator": {
    "type": "bucket",
    "sorting_keys": [["text", "num_tokens"]],
    "batch_size" : 80
  },
  "trainer": {
    "num_epochs": 200,
    "grad_norm": 1.0,
    "patience": 10,
    "validation_metric": "+fscore-at-0.5",
    "cuda_device": 0,
    "optimizer": {
      "type": "adadelta",
      "rho": 0.95
    }
  }
}
