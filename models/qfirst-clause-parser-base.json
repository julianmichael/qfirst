{
  "dataset_reader": {
      "type": "qfirst_qasrl",
      "instance_type": "verb",
      "token_indexers": {
          "elmo": {
            "type": "elmo_characters"
          }
      },
      "min_answers": 0,
      "min_valid_answers": 0
  },
  "model": {
      "type": "qfirst_parser",
      "clause_mode": true
  },
  "iterator": {
    "type": "bucket",
    "sorting_keys": [["text", "num_tokens"]],
    "batch_size" : 1
  }
}
