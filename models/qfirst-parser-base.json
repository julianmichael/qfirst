{
  "dataset_reader": {
      "type": "qfirst_qasrl",
      "instance_type": "verb",
      "token_indexers": {
          "elmo": {
            "type": "elmo_characters"
          }
      },
      "min_answers": 3,
      "min_valid_answers": 3
  },
  "model": {
      "type": "qfirst_parser",
      "metric": {
          "type": "filter_tuning",
          "target": "q-e2e-f1",
          "question_thresholds": [0.02, 0.05, 0.1, 0.25, 0.5],
          "span_thresholds": [0.1, 0.2, 0.35, 0.5, 0.65],
          "invalid_thresholds": [0.1, 0.25, 0.4, 0.6, 0.8]
      }
  },
  "iterator": {
    "type": "bucket",
    "sorting_keys": [["text", "num_tokens"]],
    "batch_size" : 1
  }
}
